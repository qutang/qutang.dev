    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "front-matter": "^3.2.1",
    "glob": "^7.1.6",
    "highlight.js": "^10.0.3",
